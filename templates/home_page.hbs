<section class="sections">
  <div class="container">
    <div class="flex-grid-section">
      <script>
        const truncate = (str, max, suffix) => str.length < max ? str : `${str.substr(0, str.substr(0, max - suffix.length).lastIndexOf(' '))}${suffix}`;
      </script>
      <div class="flex-row">
        {{#each categories }}
        {{#each sections }}
        {{#each articles}}
        <div class="flex-col-lg-6">
          <div class="item-img-left">
            <div class="item_img">
              <div class="img_wrap">
                <svg xmlns="http://www.w3.org/2000/svg" width="150" height="150">
                  <rect x="2" y="2" width="146" height="146" style="fill:#DEDEDE;" /><text x="50%" y="50%"
                    font-size="18" text-anchor="middle" alignment-baseline="middle" font-family="monospace, sans-serif"
                    fill="#555555">300x300</text>
                </svg>

              </div>

            </div>
            <div class="item-text">
              <div class="article-title"><a href="{{url}}">{{title}}</a></div>
              <div class="article-description">
                <script>
                  var article = '\{{ body }}';
                  document.body.appendChild(truncate(article.replace(/(<([^>]+)>)/gi, ""), 120, '...'));
                </script>
              </div>
            </div>
          </div>
          <div class="space-15"></div>
        </div>
        {{/each}}
        {{/each}}
        {{/each}}

      </div>
    </div>
    <div class="pagination">{{pagination}}</div>
  </div>
</section>

<script>
  document.addEventListener('DOMContentLoaded', function ($) {



    // Helper Functions

    // Hide and Show functions
    function show(el) {
      var el = document.querySelectorAll(el);
      for (i = 0; i < el.length; i++) {
        if (el[i].style.display == 'none') {
          el[i].style.display = 'block';
        }
      }
      return false;
    }
    function hide(el) {
      var el = document.querySelectorAll(el);
      for (i = 0; i < el.length; i++) {
        if (el[i].style.display == 'block') {
          el[i].style.display = 'none';
        }
      }
      return false;
    }

    // Fade in Function
    function fadeIn(el) {
      el.style.opacity = 0;
      el.style.display = "block";
      var tick = function () {
        el.style.opacity = +el.style.opacity + 0.05;
        if (+el.style.opacity < 1) {
          (window.requestAnimationFrame && requestAnimationFrame(tick)) || setTimeout(tick, 16)
        }
      };
      tick();
    }

    // Remove and Add Class functions
    function addClass(el, className) {
      var el = document.querySelectorAll(el);

      for (i = 0; i < el.length; i++) {
        if (el.classList) {
          el[i].classList.add(className);
        } else {
          el[i].className += ' ' + className;
        }
      }
    }

    function removeClass(el, className) {
      var el = document.querySelectorAll(el);

      for (i = 0; i < el.length; i++) {
        if (el[i].classList) {
          el[i].classList.remove(className);
        } else {
          el[i].className = el.className.replace(new RegExp('(^|\\b)' + className.split(' ').join('|') + '(\\b|$)', 'gi'), ' ');
        }
      }
    }

    // Main function for swapping tabs / content on click
    function tabMagic(el) {
      [].forEach.call(el, function (el) {
        el.addEventListener('click', function () {
          hide(".tab_content");

          var activeTab = this.getAttribute("rel");
          fadeIn(document.querySelector("#" + activeTab));

          removeClass("ul.tabs li", "active");
          addClass("li[rel^='" + activeTab + "']", "active");

          removeClass(".tab_drawer_heading", "d_active");
          addClass(".tab_drawer_heading[rel^='" + activeTab + "']", "d_active");
        })
      })
    }

    var tabContent = document.getElementById("tab1");
    if (tabContent) {
      // Hide the content and show the first tab
      hide(".tab_content");
      //var parentElement = document.getElementsById("common_questions");
      fadeIn(document.querySelector("#tab1"));

      //Desktop Tabs
      var tabLi = document.querySelectorAll("ul.tabs li");
      tabMagic(tabLi);

      // Drawer Tabs
      var drawer = document.querySelectorAll(".tab_drawer_heading");
      tabMagic(drawer);
    }

    var languageSelector = document.getElementById("language-selector-wrapper");
    if (languageSelector) {
      // Get the element
      var elem = document.querySelector('#language-selector-wrapper > .language-selector');
      var elem2 = document.querySelector('#filter-language-selector');
      // Create a copy of it
      var clone = elem.cloneNode(true);
      // Inject it into the DOM
      elem2.appendChild(clone);
    }

  });
</script>